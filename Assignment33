package mouni;
import java.util.Scanner;
public class Assignment33 {

	public static void main(String[] args) {
	

				Scanner sc=new Scanner(System.in);
			
				studentResult st1=new studentResult("D",404,73);
				st1.showStudent();
				studentResult st2=new studentResult("M",403,73);
				st2.showStudent();
				studentResult st3=new studentResult("s",400,74);
				st3.showStudent();
				sc.close();
			}
		}

		class studentResult{
			String studentName;
			int rollno;
			float marks;

			studentResult(String studentName,int rollno,float marks){
				
				this.studentName=studentName;
				this.rollno=rollno;
				this.marks=marks;
				
				}
				void showStudent(){
					System.out.println(""+studentName);
					System.out.println(""+rollno);
					System.out.println(""+marks);
				}
		}
